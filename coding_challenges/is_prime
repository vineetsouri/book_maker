#include <iostream>
using namespace std;

int main()
{
  int n;
  int tof = 0;
  cin >> n;
  int m = (n/2)+1;
  for(int i = 2; i < m; i++)
  {
    if(n%i == 0)
    {
      tof = 1;
      break;
    }
  }
  if(tof == 0)
  {
    cout << "The given positive integer is prime.";
  }
  if(tof == 1)
  {
    cout << "The given positive integer is composite.";
  }
}